{
  "name": "@material-tailwind/html",
  "version": "3.0.0-beta.7",
  "description": "Material Tailwind is an open-source library that uses the power of Tailwind CSS and React to help you build unique web projects faster and easier. The stunning design inspired by Material Design is a bonus!",
  "type": "module",
  "exports": {
    "import": "./dist/material-tailwind.esm.js",
    "require": "./dist/material-tailwind.cjs.js",
    "default": "./dist/material-tailwind.umd.js"
  },
  "main": "./dist/material-tailwind.cjs.js",
  "module": "./dist/material-tailwind.esm.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/material-tailwind.umd.min.js",
  "browser": "dist/material-tailwind.umd.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/material-tailwind.git"
  },
  "keywords": [
    "ui",
    "material",
    "tailwind",
    "components",
    "tailwindcss",
    "javascript"
  ],
  "author": "Creative Tim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/material-tailwind/issues"
  },
  "homepage": "https://www.material-tailwind.com/docs/v3/html/button",
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup": "^4.34.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "tslib": "^2.8.1"
  }
}
